<div class="col-md-8 offset-md-1">
  <h2>ジャンル編集</h2>
  <div class="edit_genres">
    <%= form_with model: [:admin, @genre], local:true do |f| %>
      <div class="form-group">
        <label for="genre_name">ジャンル名</label>
        <%= f.text_field :name %>
        <button class="btn btn-success"><% f.submit "編集" %>変更を保存</button>
      </div>
    <% end %>
</div>